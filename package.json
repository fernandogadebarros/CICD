{
    "dependencies": {
        "next": "^13.1.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "scripts": {
        "dev": "next dev",
        "build": "rm -rf build && next build && next export && mv ./out ./build",
        "start": "next start",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "deploy": "npx vercel ./build --name projeto-cicd",
        "deploy:prod": "yarn build && vercel ./build --confirm --no-clipboard --token=$VERCEL_TOKEN --prod",
        "deploy:preview": "node ./scripts/pullRequestPreview"
    },
    "devDependencies": {
        "@types/node": "18.11.18",
        "@types/react": "18.0.26",
        "eslint": "8.30.0",
        "eslint-config-next": "13.1.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "prettier": "^2.8.1",
        "typescript": "4.9.4"
    }
}
